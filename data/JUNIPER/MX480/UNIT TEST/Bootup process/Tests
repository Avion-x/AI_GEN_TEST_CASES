[
{
    "testname": "Test Bootup Time", 
    "testcase": {
        "testname": "Test Bootup Time",
        "objective": "Validate if the bootup time of the router is within expected limit", 
        "steps": ["Power on the router", "Note start time", "Router finish booting", "Note end time", "Validate if bootup time is less than 5 minutes"]
    },
    "testscript": {
        "testname": "Test Bootup Time",
        "objective": "Validate if the bootup time of the router is within expected limit",
        "file_name": "test_bootup_time.py",
        "init_scripts": ["import time"],
        "script": """
                    start_time = time.time()
                    print("Router powered on") 
                    time.sleep(300)
                    end_time = time.time()
                    duration = end_time - start_time  
                    assert duration < 300
                """,
        "run_command": "python test_bootup_time.py",
        "expected_result": "Test should pass"
    }
},
